{
    "name": "kduma/bulk-generator",
    "description": "Bulk Document Generator",
    "keywords": [
        "kduma",
        "bulk-generator"
    ],
    "homepage": "https://github.com/kduma/bulk-generator",
    "license": "GPL-2.0-or-later",
    "type": "library",
    "authors": [
        {
            "name": "Krystian Duma",
            "email": "git@krystian.duma.sh",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "mpdf/mpdf": "^8.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "rector/rector": "^2.0",
        "symplify/easy-coding-standard": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "Kduma\\BulkGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kduma\\BulkGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "rector": "vendor/bin/rector",
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi"
    },
    "config": {
        "sort-packages": true
    }
}